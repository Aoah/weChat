
.containter {
    height: 100%;
    width: 100%;
    display: flex;
    flex-direction: row;
    justify-content: space-between;
}
.sidle {
    flex: 2;
    height: 100%;
    background: #f5f5f5;
}
.sidle ul {
    width: 100%;
    height: 100%;
}
.sidle li {
    display: block;
    line-height: 110rpx;
    text-align: center;
    width: 100%;
    height: 110rpx;
}

.li1 {
    border-right: 1px solid #d7d7d7;
    border-bottom: 1px solid #d7d7d7;
}

.li1change {
    border-bottom: 1px solid #d7d7d7;
    color: red;
    background: white;
}

.contain {
   overflow: hidden;
    flex: 5;
    padding-left: 20rpx;
    height: 100%;
}

.logo {
    white-space:nowrap;
    width:100%;
    height: 220rpx;
    display: flex;
    flex-direction: row;
    align-items: center;
    margin-bottom: 20rpx;
}

.block {
   text-overflow: ellipsis;
    overflow: hidden;
    text-align: center;
    display:inline-block;
    margin-top:40rpx;
    margin-right: 20rpx;
    width: 140rpx;
    height: 140rpx;
    border-radius: 5px;
    border: 1px solid #d7d7d7;
}

.block image {
  display:block;
    width: 100%;
    height:65%;
}
.block text {
   text-overflow: ellipsis;
    width:90%;
    font-size: 20rpx;
}

.bb{
     display:block;
     width:100%;
     overflow: hidden;
}
.bbtitle{
  clear:both;
  font-size: 22rpx;
  display:block;
  margin-bottom:60rpx;
}
.outCon1{
     overflow: hidden;
}
.con1{
  margin-bottom:54rpx;
  float:left;
  margin-left:30rpx;
  width:115rpx;
  height:135rpx;
  text-align:center;
}
.con1 image{
    width:100%;
    height:65%;
}
.con1 text{
      display: block;
     font-size:22rpx;
}
